    "paths": {
        "/posts": {
            "get": {
                "summary": "Get all form posts",
                "operationId": "getPosts",
                "description": "Get all posts",
                "responses": {
                    "200": {
                        "description": "OK",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "id": {
                                            "type": "integer",
                                            "description": "The post id",
                                            "example": 1
                                        },
                                        "name": {
                                            "type": "string",
                                            "description": "The post title",
                                            "example": "Form Template 1"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    },
